mostlyclean-libtool:
	-rm -f *.lo

clean-libtool:
	-rm -rf .libs _libs


#all-am: Makefile $(LTLIBRARIES) $(PROGRAMS) $(HEADERS)
#all-am: link_header $(LTLIBRARIES) $(PROGRAMS) $(HEADERS)
all-am: $(LTLIBRARIES) $(PROGRAMS) $(HEADERS)


#link_header: 
#	@ln -sf $(PROJ_ROOT)/src/sysapps/apps/hydra/event/event_prototype.h

BIN	= lighttpd$(EXEEXT)
BINDIR	= $(PROJ_INSTALL)/usr/local/bin
#BINDIR	= /data/nfs/wvc2300/httpd
CONF	= lighttpd.conf
TMPCONF	= httpd.conf
TMPDIR	= /tmp
LIBDIR = $(PROJ_INSTALL)/usr/lib/lighttpd
#LIBDIR	= $(BINDIR)/lib
INSTALL_LIB = mod_accesslog.so mod_access.so mod_dirlisting.so mod_cgi.so mod_indexfile.so mod_staticfile.so mod_auth.so
install: $(CONF)
	$(STRIP) $(BIN)
	install -D $(BIN) $(BINDIR)/$(BIN)
	install -D $(CONF) $(BINDIR)/$(CONF)
	ln -sf $(TMPDIR)/$(TMPCONF) $(BINDIR)/$(TMPCONF)
	for i in `echo $(INSTALL_LIB)`; do \
		$(STRIP) .libs/$$i; \
		install -D .libs/$$i $(LIBDIR)/$$i;\
	done

gpl-clean:
	rm -f *.c


